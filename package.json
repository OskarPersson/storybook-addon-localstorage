{
  "name": "@alexgorbatchev/storybook-addon-localstorage",
  "version": "1.0.2",
  "description": "A Storybook v8 addong for mocking and displaying `localStorage` values",
  "keywords": [
    "storybook-addons",
    "localStorage",
    "test",
    "mocking"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgorbatchev/storybook-addon-localstorage"
  },
  "authors": [
    "Alex Gorbatchev <alex.gorbatchev@gmail.com>"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.mjs",
      "require": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    },
    "./decorator": {
      "types": "./dist/decorator.d.ts",
      "require": "./dist/decorator.mjs",
      "import": "./dist/decorator.mjs"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "require": "./dist/preview.mjs",
      "import": "./dist/preview.mjs"
    },
    "./manager": "./dist/manager.mjs",
    "./preset": "./dist/preset.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "decorator": [
        "dist/decorator.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.png",
    "!src/**/*"
  ],
  "scripts": {
    "storybook": "storybook dev --port 6006",
    "build": "tsup"
  },
  "devDependencies": {
    "@storybook/components": "^8.1.5",
    "@storybook/manager-api": "^8.1.5",
    "@storybook/preview-api": "^8.1.5",
    "@storybook/types": "^8.1.5",
    "@storybook/react-vite": "^8.1.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "storybook": "^8.1.5",
    "tsup": "^8.1.0",
    "vite": "^5.2.0",
    "use-local-storage-state": "^19.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "localStorage",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
